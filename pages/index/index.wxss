  height: fit-content;
  /* display: flex;
  flex-direction: column; */

}
.second-page-player {
  z-index: 999;
  position: fixed;
  width: 100%;
  bottom: 0px;
  transform: scaleY(0);
  transform-origin: bottom;
  transition: transform 0.1s ease;
  box-sizing: border-box;
}
.second-page-player-icon {
  box-sizing: border-box;
  position: absolute;
  left: -40px;
  bottom: 0px;
  width: 220px;
  height: 150px;
}
.second-page-player-record {
  box-sizing: border-box;
  position: absolute;
  right: 5px;
  top: -50px;
  width: 100px;
  height: 100px;
  z-index: 0;
  animation: rotate 30s linear infinite;
}
.animation {
  animation-play-state: paused;
}
@keyframes rotate {
  0% {
    transform: rotate(0);
  }
  25% {
    transform: rotate(90deg);
  }
  50% {
    transform: rotate(180deg);
  }
  75% {
    transform: rotate(270deg);
  }
  100% {
    transform: rotate(360deg);
  }
}
.second-page-player-background {
  box-sizing: border-box;
  top: 0px;
  position: absolute;
  left: 0px;
  height: 100%;
  width: 100%;
  background-color: #eae9e3;
}
.second-page-player-content {
