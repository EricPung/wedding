.guideline-arrow {
  display: block;
  height: 10px;
  width: 100%;
  position: absolute;
  left: 0px;
  filter: opacity(0.1);
  animation: fadeInOut 3s infinite;
}
.arrow1 {
  top: -18px;
  animation-delay: 0s;
}
.arrow2 {
  top: -10px;
  animation-delay: 0.5s;
}
.arrow3 {
